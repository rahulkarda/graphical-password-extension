{
   "name": "Graphical Password Extension",
   "manifest_version": 2,
   "version": "0.1.0",
   "content_scripts": [
      {
         "matches": ["<all_urls>"],
         "js": ["content.js"]
      }
   ],
   "background": {
      "scripts": ["background.js"]
   },
   "permissions": ["tabs"],
   "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "./popup/index.html",
      "default_title": "Graphical Password Extension"
   }
}